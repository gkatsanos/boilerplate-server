{
  "name": "isawyou-server",
  "version": "1.1.0",
  "description": "The server behind",
  "author": "Georgios Katsanos <george@gplus.gr>",
  "main": "src/index.js",
  "private": false,
  "engines": {
    "node": "^8.0.0",
    "yarn": "^1.2.1",
    "npm": "^5.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive src/tests",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "postinstall": "yarn seed",
    "deploy": "sh ./deploy.sh",
    "docs": "apidoc -i src -o docs",
    "postdocs": "opn docs/index.html",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit",
    "seed": "node seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gkatsanos/isawyou.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "istanbul",
    "nyc",
    "eslint",
    "Travis CI",
    "coveralls",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "boom": "^6.0.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv-safe": "^4.0.4",
    "express": "^4.15.2",
    "express-session": "^1.15.5",
    "express-validator": "^4.1.1",
    "faker": "^4.1.0",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "jwt-simple": "0.5.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "moment-timezone": "^0.5.13",
    "mongod": "^2.0.0",
    "mongodb": "^2.2.31",
    "mongoose": "^4.9.7",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.8.1",
    "nodemailer": "^4.1.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^3.0.0",
    "pm2": "^2.4.6",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.3",
    "opn-cli": "^3.1.0",
    "sinon": "^3.0.0",
    "sinon-chai": "^2.10.0",
    "supertest": "^3.0.0"
  },
  "license": "MIT"
}
